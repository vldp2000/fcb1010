def clearScreenDebug():
  global gDebugFlag
  global gDebugMessageCounter
  if gDebugFlag == 'Debug':
    print("\n" * 2)
    print(f'               >> ----- {gDebugMessageCounter} -------<<' )
    gDebugMessageCounter = gDebugMessageCounter + 1




